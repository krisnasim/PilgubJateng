<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="id.mdh.pilgubjateng.fragment.RegistrationTPSFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="NRP"
                android:layout_alignParentTop="true"
                android:layout_alignStart="@+id/editText2"
                android:layout_marginTop="17dp" />

            <EditText
                android:id="@+id/editText2"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="34dp"
                android:layout_below="@id/textView3"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="Nomor NRP" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Nama Polri"
                android:layout_marginTop="14dp"
                android:layout_below="@+id/editText2"
                android:layout_alignStart="@+id/editText3" />

            <EditText
                android:id="@+id/editText3"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="Nama Lengkap"
                android:layout_below="@+id/editText2"
                android:layout_alignStart="@+id/editText4"
                android:layout_marginTop="30dp" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Jabatan"
                android:layout_below="@+id/editText3"
                android:layout_alignStart="@+id/editText4"
                android:layout_marginTop="14dp" />

            <EditText
                android:id="@+id/editText4"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="Nama Jabatan"
                android:layout_below="@+id/editText3"
                android:layout_alignStart="@+id/editText2" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Pangkat"
                android:layout_marginTop="14dp"
                android:layout_below="@+id/editText4"
                android:layout_alignStart="@+id/editText3" />

            <EditText
                android:id="@+id/editText5"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="Nama Pangkat"
                android:layout_below="@+id/editText4"
                android:layout_alignStart="@+id/editText2" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Kabupaten"
                android:layout_marginTop="14dp"
                android:layout_below="@+id/editText5"
                android:layout_alignStart="@+id/editText3" />

            <EditText
                android:id="@+id/editText6"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="Nama Kabupaten"
                android:layout_below="@+id/editText5"
                android:layout_alignStart="@+id/editText2" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Kecamatan"
                android:layout_marginTop="14dp"
                android:layout_below="@+id/editText6"
                android:layout_alignStart="@+id/editText3" />

            <EditText
                android:id="@+id/editText7"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="Nama Kecamatan"
                android:layout_below="@+id/editText6"
                android:layout_alignStart="@+id/editText2" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Kelurahan/Desa"
                android:layout_marginTop="14dp"
                android:layout_below="@+id/editText7"
                android:layout_alignStart="@+id/editText3" />

            <EditText
                android:id="@+id/editText8"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="Nama Keluarahan/Desa"
                android:layout_below="@+id/editText7"
                android:layout_alignStart="@+id/editText2" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Nomor TPS"
                android:layout_marginTop="14dp"
                android:layout_below="@+id/editText8"
                android:layout_alignStart="@+id/editText3" />

            <EditText
                android:id="@+id/editText9"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="Nomor TPS"
                android:layout_below="@+id/editText8"
                android:layout_alignStart="@+id/editText2" />

            <Button
                android:id="@+id/submit_regis_button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/editText9"
                android:background="@color/colorAccent"
                android:textColor="#ffffff"
                android:layout_marginTop="30dp"
                android:layout_marginBottom="30dp"
                android:layout_centerHorizontal="true"
                android:text="Kirim"/>

        </RelativeLayout>
    </ScrollView>
</LinearLayout>
